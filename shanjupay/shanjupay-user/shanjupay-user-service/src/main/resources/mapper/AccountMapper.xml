<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shanjupay.user.mapper.AccountMapper">
<select id="selectAccountByMobile" parameterType="java.lang.String" resultType="java.lang.Integer">
    select count(*) from account where MOBILE=#{mobile}
</select>
</mapper>
